# Default configuration for Door Monitor
# These values will be used if not configured via menuconfig

# WiFi Configuration (CHANGE THESE BEFORE BUILDING!)
CONFIG_DOOR_WIFI_SSID="CHANGE_ME"
CONFIG_DOOR_WIFI_PASSWORD="CHANGE_ME"

# Server Configuration
CONFIG_DOOR_SERVER_IP="192.168.1.100"
CONFIG_DOOR_SERVER_PORT=8080

# Enable WiFi
CONFIG_ESP32_WIFI_ENABLED=y